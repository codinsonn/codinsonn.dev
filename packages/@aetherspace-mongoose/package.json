{
    "name": "@aetherspace/mongoose",
    "version": "0.0.1",
    "private": true,
    "license": "SEE LICENSE IN LICENSE.md",
    "description": "The official aetherspace package for using mongoose and mongodb",
    "aetherspace": {
        "relatedWorkspaces": [
            "aetherspace"
        ],
        "requiredEnvVars": [
            "MONGODB_URI"
        ],
        "resolutions": {
            "@mongodb-js/zstd": false,
            "@aws-sdk/credential-providers": false,
            "snappy": false,
            "aws4": false,
            "mongodb-client-encryption": false,
            "kerberos": false,
            "supports-color": false
        }
    },
    "scripts": {
        "lint": "cd ../.. && eslint \"packages/@aetherspace-mongoose/**/*.{ts,tsx}\""
    },
    "dependencies": {
        "mongoose": "^7.4.2"
    }
}
